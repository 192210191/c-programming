#include<stdio.h>
int main()
{
	int i,start,end,j,count;
	printf("enter the values of start and end");
	scanf("%d%d",&start,&end);
	for(i=start;i<=end;i++)
	{
	count=0;
		for(j=1;j<=i;j++)
		{
			if(i%j==0)
			{
			count=count+1;
			}
		}
	
	if(count==2)
	{
	
	printf("prime number:%d\n",i);
	}
}
return 0;
}
